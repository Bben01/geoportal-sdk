{
    "name": "geoportal-sdk",
    "version": "2.0.1",
    "SDK2DVersion": "2.0.1",
    "SDK3DVersion": "2.0.1",
    "description": "French Geoportal SDK based on OpenLayers (2D) and iTowns (3D) libraries",
    "main": "dist/2d/GpSDK2D.js, dist/3d/GpSDK3d.js",
    "scripts": {
        "setup": "npm install",
        "clean": "echo \"Warning: target not yet implemented!\" && exit 0",
        "test:serve": "webpack-dev-server --hot --config webpack.test.serve.js",
        "doc:serve": "webpack-dev-server --config webpack.config.3d --content-base jsdoc/ --port 9001 --open 'google-chrome'",
        "sample:3d:serve": "webpack-dev-server --config webpack.config.3d --env.development --https --open-page samples/index-3d-map.html --content-base . --output-public-path '/dist/3d/' --port 9001 --open 'google-chrome'",
        "sample:2d:serve": "webpack-dev-server --config webpack.config.2d --env.development --https --open-page samples/index-2d-map.html --content-base . --output-public-path '/dist/2d/' --port 9001 --open 'google-chrome'",
        "stat:2d": "webpack --config webpack.config.2d --profile --json > map-2d.json",
        "stat:3d": "webpack --config webpack.config.3d --profile --json > map-3d.json",
        "build": "npm-run-all --print-label --print-name build:*:*",
        "build:dev": "npm run build:2d:dev; npm run build:3d:dev",
        "build:prod": "npm run build:2d:prod; npm run build:3d:prod",
        "build:src": "npm run build:2d; npm run build:3d",
        "build:3d": "npm-run-all --print-label --print-name build:3d:*",
        "build:3d:src": "webpack --config webpack.config.3d",
        "build:3d:dev": "webpack --config webpack.config.3d --env.development",
        "build:3d:prod": "webpack --config webpack.config.3d --env.production",
        "build:2d": "npm-run-all --print-label --print-name build:2d:*",
        "build:2d:src": "webpack --config webpack.config.2d",
        "build:2d:dev": "webpack --config webpack.config.2d --env.development",
        "build:2d:prod": "webpack --config webpack.config.2d --env.production"
    },
    "repository": {
        "type": "git",
        "url": "https://github.com/IGNF/geoportal-sdk.git"
    },
    "keywords": [
        "openlayers",
        "itowns",
        "geoportail",
        "webservice",
        "javascript"
    ],
    "author": "IGNF",
    "license": "CECILL-B",
    "bugs": {
        "url": "https://github.com/IGNF/geoportal-sdk/issues"
    },
    "homepage": "https://geoservices.ign.fr/documentation/utilisation_web/sdk.html",
    "dependencies": {
        "itowns": "2.3.0"
    },
    "devDependencies": {
        "babel-core": "^6.26.3",
        "babel-loader": "^7.1.2",
        "babel-preset-env": "^1.6.1",
        "chai": "^4.2.0",
        "chalk": "^2.4.2",
        "copy-webpack-plugin": "^4.6.0",
        "css-loader": "^0.28.10",
        "eslint": "^4.18.2",
        "eslint-config-standard": "^11.0.0",
        "eslint-loader": "^2.1.2",
        "eslint-plugin-import": "^2.16.0",
        "eslint-plugin-node": "^6.0.1",
        "eslint-plugin-promise": "^3.7.0",
        "eslint-plugin-standard": "^3.0.1",
        "exports-loader": "^0.7.0",
        "expose-loader": "^0.7.5",
        "extract-text-webpack-plugin": "^3.0.2",
        "geoportal-extensions-itowns": "https://raw.githubusercontent.com/IGNF/geoportal-extensions/update-ol5/build/scripts/release/geoportal-extensions-itowns-2.2.0.tgz",
        "geoportal-extensions-openlayers": "https://raw.githubusercontent.com/IGNF/geoportal-extensions/update-ol5/build/scripts/release/geoportal-extensions-openlayers-3.0.0.tgz",
        "handlebars": "^4.1.0",
        "handlebars-layouts": "^3.1.4",
        "html-webpack-plugin": "^3.2.0",
        "jsdoc-webpack-plugin": "0.0.1",
        "loglevel": "^1.6.1",
        "mocha": "^5.2.0",
        "mocha-loader": "^1.1.3",
        "mocha-webpack": "^1.1.0",
        "npm-run-all": "^4.1.5",
        "ol": "^5.3.0",
        "ol-mapbox-style": "^4.2.1",
        "path": "^0.12.7",
        "proj4": "^2.4.4",
        "replace-bundle-webpack-plugin": "^1.0.0",
        "requirejs": "^2.3.6",
        "speed-measure-webpack-plugin": "^1.3.0",
        "string-template": "^1.0.0",
        "style-loader": "^0.20.2",
        "three": "^0.93.0",
        "three.meshline": "^1.2.0",
        "uglify-es": "github:mishoo/UglifyJS2#harmony",
        "uglifyjs-webpack-plugin": "^1.3.0",
        "url-loader": "^1.1.2",
        "webpack": "^3.11.0",
        "webpack-dev-server": "^2.11.1"
    }
}
